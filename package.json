{
  "name": "freestream",
  "version": "1.4.0",
  "description": "Free multi-streaming desktop app â€” fan out your stream to Twitch, YouTube, Facebook, TikTok and more",
  "author": "Dhillon Kannabhiran",
  "license": "MIT",
  "main": "dist/main/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "start": "electron dist/main/main/index.js",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "test": "vitest --run",
    "test:main": "vitest --run --project main",
    "test:renderer": "vitest --run --project renderer",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "test:e2e": "npm run build && npx playwright test",
    "test:e2e:headed": "npm run build && npx playwright test --headed",
    "test:e2e:only": "npx playwright test"
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "keytar": "^7.9.0",
    "mpegts.js": "^1.8.0",
    "node-media-server": "^2.6.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^4.0.18"
  },
  "build": {
    "extends": "electron-builder.yml"
  }
}
